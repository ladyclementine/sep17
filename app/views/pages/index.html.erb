    <!-- Header -->
    <header id="top" class="header">
        <div class="text-vertical-center">
            <h1><%= current_week[:name] %></h1>
            <h3><%= current_week[:description] %></h3>
            <br>
            <%= link_to new_user_registration_path, class:'btn btn-dark btn-lg' do %>
               Inscreva-se
            <% end %>
            <%= link_to new_user_session_path, class:'btn btn-dark btn-lg' do %>
               Entrar no Painel
            <% end %>
        </div>
    </header>
    <!-- Services -->
