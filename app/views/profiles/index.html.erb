
            <!-- ============================================================== -->
            <!-- Start right Content here -->
            <!-- ============================================================== -->
            <div class="content-page">
                <!-- Start content -->
                <div class="content">
                    <div class="container">

                        <div class="row">
                            <div class="col-sm-12">
                                <div class="timeline">

                                <% @eventsDays.each do |days| %>
                                    <article class="timeline-item alt">
                                        <div class="text-right">
                                            <div class="time-show first">
                                                <a href="#" class="btn btn-custom w-lg"><%= days.strftime("%A, %b %d") %> </a>
                                            </div>
                                        </div>
                                    </article>
                                     <% @scheduleHash[days].each do |schedule| %>
                                     
                                    <article class="timeline-item alt">
                                        <div class="timeline-desk">
                                            <div class="panel">
                                                <div class="panel-body">
                                                    <span class="arrow-alt"></span>
                                                    <span class="timeline-icon bg-danger"><i class="zmdi zmdi-circle"></i></span>
                                                    <h4 class="text-danger">1 hour ago</h4>
                                                    <p class="timeline-date text-muted"><small><%=schedule.start_time.strftime("%H : %M")%></small></p>
                                                    <p> <%=schedule.event.name%> <%= schedule.event_id %></p>
                                                </div>
                                                  
                                            </div>
                                             <% cart_action = @events.find(schedule.event_id).cart_action(@user) %>

                                       <%=link_to "", class: "button", data: {target: cart_action, addUrl: add_to_cart_path(@events.find(schedule.event_id), :method => :put), removeUrl: remove_from_cart_path(@events.find(schedule.event_id),:method => :put)} do%>
    <i class="fi-shopping-cart"></i>
    <span><%=cart_action%></span> Cart
  <%end%>
  
                                        </div>
                                    </article>
                                     
                                      <% end %>
                                    <% end %>

                                </div>
                            </div>
                        </div>
                        <!-- end row -->


                    </div> <!-- container -->

                </div> <!-- content -->

                <footer class="footer">
                    2016 Â© Adminto.
                </footer>

            </div>


